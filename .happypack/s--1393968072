function mapStateToProps(state) {
  const aboutusdata = state;
  return { aboutusdata };
}

function mapDispatchToProps(dispatch, props) {
  return {
    getAboutUs: token_val => {
      dispatch((0, _userProfile_action.getAboutUs)(token_val));
    },
    editAboutUs: (token_val, editaboutdata) => {
      dispatch((0, _userProfile_action.editAboutUs)(token_val, editaboutdata));
    },
    editNameSurname: (token_val, name, surname) => {
      dispatch((0, _userProfile_action.editNameSurname)(token_val, name, surname));
    }
  };
}

exports.default = (0, _reactRedux.connect)(mapStateToProps, mapDispatchToProps)(UserProfile);